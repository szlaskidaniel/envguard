{
  "$schema": "https://json-schema.org/draft-07/schema",
  "title": "EnvGuard Configuration",
  "description": "Configuration file for EnvGuard - keep your environment variables in sync",
  "ignoreVars": [
    "MY_COMPANY_VAR",
    "PLATFORM_PROVIDED_VAR"
  ],
  "strict": false,
  "exclude": [
    "**/tmp/**",
    "**/cache/**",
    "**/node_modules/**",
    "**/dist/**",
    "**/test/**"
  ]
}
